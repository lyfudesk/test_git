##获取最近git仓库
git fetch 

##切换到研发分支
git checkout $1

##覆盖本地分支为远程最近分支
git reset origin/$1  --hard

##切换到week分支
git checkout week

##覆盖本地分支为远程最新分支
git reset origin/week --hard

##merge研发分支
git merge $1
##判断是否产生冲突
if [ $? -eq 0 ];then
  git push origin week
else
  echo " ________________________"
  echo "|                        |"
  echo "|      merge失败         |"
  echo "|      出现冲突          |"
  echo "|   请手动解决冲突       |"
  echo "|________________________|"
fi
